#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main(){
	int highest, lowest, CMF, range, loop;
	int tab = 7;
	int freq = 0;
	float classSize, mean;
	
	cout << "Enter the number of scores " << endl;
	cin >> loop; 
	
	int array[loop];	
	float LL[loop], UL[loop], CM[loop], F[loop], MF[loop];
	for(int i = 0; i < loop; i++){
		cout << i+1 << ". ";
		cin >> array[i];
	} 
	
	highest = array[0];
	lowest =  array[0];
	for(int i = 0; i < loop; i++){ 
		if(array[i] > highest){
			highest = array[i];	
		}
		if(array[i] < lowest){
			lowest = array[i];
		}   
	} 
	
	range = highest - lowest;
	classSize = log10(loop) * 3.322 + 1;
	classSize = round(classSize);
	int column = 0;
    int x = lowest;
    while(x < highest){
    	x = x + classSize;
		column++;
	}
	
	LL[0] = lowest;
	UL[0] = classSize;
	cout << "LL" << setw(tab) << "UL" << setw(tab) << "CM" << setw(tab) << "F" << setw(tab) << "MF" << endl;
	for(int i = 0; i <= column ; i++){
		UL[i] = LL[i] + classSize - 1;
		CM[i] =(LL[i] + UL [i]) / 2;
		F[i] = 0;
		for(int j = 0; j <= loop; j++) {
	        if(array[j] >= LL[i] && array[j] <= UL[i]) {
	           F[i] = F[i]+1;
	        }
		}
		MF[i] = CM[i] * F[i];
		cout << LL[i] << setw(tab) << UL[i] << setw(tab) << CM[i] << setw(tab) << F[i] << setw(tab) << MF[i] <<endl;
		LL[i+1] = LL[i] + classSize;
		CMF = CMF + MF[i];
	}
	
	mean = (float)CMF / loop;
	cout << "Highest Score: " << highest << endl;
	cout << "Lowest ScoreL " << lowest << endl;
	cout << "Class Size: " << classSize << endl;
	cout << "Range: " << range << endl;
	cout << "Summation of CMF: " << CMF << endl;
	cout << "MEAN for Grouped Data: " << mean << endl;
	return 0;
}
